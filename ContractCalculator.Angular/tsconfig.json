{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@models": [
        "./app/models"
      ],
      "@services": [
        "./app/services"
      ],
      "@constants": [
        "./app/constants"
      ],
      "@enums": [
        "./app/enums"
      ],
      "@utils": [
        "./app/utils"
      ]
    },
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
